    "lint-staged": "^15.2.11",
    "prettier": "^3.5.1",
    "typescript": "^5.7.0",
    "vite": "^6.0.0",
    "vitest": "^4.0.16"
  },
  "dependencies": {
    "lz-string": "^1.5.0"
